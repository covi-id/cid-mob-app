{
  "name": "coviid",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "env:staging": "node src/utils/set-environment.js staging",
    "env:dev": "node src/utils/set-environment.js dev",
    "env:prod": "node src/utils/set-environment.js prod",
    "_android": "react-native run-android",
    "android:dev": "yarn env:dev && yarn _android --variant=devDebug",
    "android:dev:release": "yarn env:dev && npx react-native run-android --variant=devRelease",
    "android:dev:codepush": "code-push release-react coviid android -d Dev",
    "android:staging": "yarn env:staging && react-native run-android --variant=stagingDebug",
    "android:staging:release": "yarn env:staging && npx react-native run-android --variant=stagingRelease",
    "android:staging:codepush": "code-push release-react coviid android -d Staging",
    "android:staging:bundle": "cd android && ./gradlew bundlestagingRelease",
    "android:prod": "yarn env:prod && react-native run-android --variant=prodDebug",
    "android:prod:release": "yarn env:prod && npx react-native run-android --variant=prodRelease",
    "android:prod:bundle": "cd android && ./gradlew bundleprodRelease",
    "android:prod:codepush": "code-push release-react coviid android -d Prod",
    "_ios": "react-native run-ios",
    "ios:dev": "yarn env:dev && yarn _ios",
    "ios:staging": "yarn env:staging && yarn _ios",
    "ios:prod": "yarn env:prod && yarn _ios",
    "codepush:ios": "code-push release-react coviid ios",
    "codepush:android": "code-push release-react coviid android",
    "test": "jest --watchAll",
    "lint": "eslint **/*.js",
    "lint-fix": "eslint **/*.js --fix",
    "build:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "postinstall": "jetify"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "~10.0.0",
    "@nartc/react-native-barcode-mask": "^1.1.9",
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^6.4.0",
    "@react-native-firebase/crashlytics": "^6.4.0",
    "@react-navigation/native": "^5.0.0",
    "@react-navigation/stack": "^5.0.0",
    "axios": "^0.19.2",
    "expo": "~36.0.0",
    "expo-font": "~8.0.0",
    "expo-location": "^8.1.0",
    "expo-permissions": "^8.1.0",
    "expo-secure-store": "^8.1.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "~0.61.4",
    "react-native-appearance": "~0.3.1",
    "react-native-camera": "^3.23.1",
    "react-native-code-push": "^6.2.0",
    "react-native-device-info": "^5.5.6",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-snackbar": "^2.2.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "9.13.3",
    "react-native-unimodules": "^0.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "~24.9.0",
    "babel-preset-expo": "~8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "~24.9.0",
    "jest-expo": "~36.0.1",
    "metro-react-native-babel-preset": "~0.56.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "~16.9.0"
  }
}
