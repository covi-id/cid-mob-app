{
  "name": "coviid",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "env:dev": "node src/utils/set-environment.js dev",
    "env:staging": "node src/utils/set-environment.js staging",
    "env:prod": "node src/utils/set-environment.js prod",
    "_android": "react-native run-android",
    "android:dev": "yarn env:dev && yarn _android --variant=devDebug",
    "android:dev:release": "yarn env:dev && yarn nodehack && npx react-native run-android --variant=devRelease",
    "android:dev:codepush": "yarn env:dev && yarn nodehack && code-push release-react coviid android -d Dev",
    "android:staging": "yarn env:staging && react-native run-android --variant=stagingDebug",
    "android:staging:release": "yarn env:staging && yarn nodehack && npx react-native run-android --variant=stagingRelease",
    "android:staging:bundle": "yarn env:staging && yarn nodehack && cd android && ./gradlew bundlestagingRelease",
    "android:staging:codepush": "yarn env:staging && yarn nodehack && code-push release-react coviid android -d Staging",
    "android:prod": "yarn env:prod && react-native run-android --variant=prodDebug",
    "android:prod:release": "yarn env:prod && yarn nodehack && npx react-native run-android --variant=prodRelease",
    "android:prod:bundle": "yarn env:prod && yarn nodehack && cd android && ./gradlew bundleprodRelease",
    "android:prod:codepush": "yarn env:prod && yarn nodehack && code-push release-react coviid android -d Prod",
    "_ios": "react-native run-ios",
    "ios:dev": "yarn env:dev && yarn _ios",
    "ios:staging": "yarn env:staging && yarn _ios",
    "ios:staging:codepush": "yarn env:staging && code-push release-react coviid-1 ios -d Staging",
    "ios:prod": "yarn env:prod && yarn _ios",
    "codepush:ios": "code-push release-react coviid ios",
    "codepush:android": "code-push release-react coviid android",
    "test": "jest --watchAll",
    "lint": "eslint **/*.js",
    "lint-fix": "eslint **/*.js --fix",
    "nodehack": "./node_modules/.bin/rn-nodeify --install --hack",
    "build:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "postinstall": "jetify && yarn nodehack"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "~10.0.0",
